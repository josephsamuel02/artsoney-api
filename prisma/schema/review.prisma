model Review {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  userId        String
  product_id    String
  storeId       String?
  profile_image String?
  product_image String[]
  rating        Int?
  review        String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
